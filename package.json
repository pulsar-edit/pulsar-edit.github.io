{
  "name": "pulsar-edit.github.io",
  "version": "1.0.0",
  "description": "A Community-led Hyper-Hackable Text Editor",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vuepress build docs",
    "clean:dev": "vuepress dev docs --clean-cache",
    "dev": "vuepress dev docs",
    "format": "prettier ./**/*.md --write --ignore-path .gitignore",
    "lint": "remark . --ignore-path .gitignore",
    "update": "pnpm dlx vp-update",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "packageManager": "pnpm@8.1.1",
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.61",
    "@vuepress/utils": "2.0.0-beta.61",
    "vue": "3.2.47",
    "vuepress": "2.0.0-beta.61",
    "vuepress-plugin-search-pro": "2.0.0-beta.201",
    "vuepress-theme-hope": "2.0.0-beta.201"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "prettier": "^2.8.7",
    "remark-cli": "^11.0.0",
    "remark-lint": "^9.1.1",
    "remark-preset-lint-consistent": "^5.1.1",
    "remark-preset-lint-markdown-style-guide": "^5.1.2",
    "remark-preset-lint-recommended": "^6.1.2",
    "remark-preset-prettier": "^2.0.1"
  },
  "nano-staged": {
    "*.md": "prettier --write"
  }
}
